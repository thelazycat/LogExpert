    <OutputPath>$(SolutionDir)..\bin\$(Configuration)</OutputPath>
    <NoWarn>CA1416;</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
